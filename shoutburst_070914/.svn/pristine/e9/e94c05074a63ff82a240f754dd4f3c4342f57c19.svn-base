.quick-access {
	position: absolute;
	left: 35%;
	top: 0px;
	z-index: 999;
	width: 200px;
}
.quick-access,
.quick-access ul,
.quick-access li {
	list-style: none;
	margin: 0px;
	padding: 0px;
}
.quick-access li .qa {
	color: #fff;
	cursor: pointer;
	padding: 5px 10px;
	background: #111;
	display: block;
	opacity: 0.3;
}
.quick-access li .qa:hover {
	background: #333;
	opacity: 1;
}

.quick-access ul {
	display: none;
	background: #333;
}
.quick-access li a,
.quick-access li a:link,
.quick-access li a:visited {
	display: block;
	color: #fff;
	padding: 5px 20px;
}
.quick-access li a:hover,
.quick-access li a:active {
	color: #2ea2cc;
	text-decoration: none;
}
.quick-access li:hover > ul {
	display: block;
}



a,
a:link,
a:visited {
	outline: none;
}

body {
	font-family: Arial, Helvetica, sans-serif;
	position: relative;
	left: 0px;
	transition: all 0.3s ease;
	overflow-x: hidden;
	background: #e1e8ed;
	padding-bottom: 30px;
}

body.page-login {
	background: #19458b;
}

body.ua-sidebar {
	left: 250px;
}











/* Typography
------------------------------------------*/

h1 {
	font-family: "Museo Sans 700", Arial, Helvetica, sans-serif;
	font-size: 20px;
	padding: 10px 0 30px 35px;
}

















/* Main Navigation
------------------------------------------*/

#main-nav {
	background-color: #19458b;
	font-size: 15px;
	position: fixed;
	left: -250px;
	transition: all 0.3s ease;
	width: 250px;
	height: 100%;
	z-index: 1000;
}
	#main-nav .logo {
		margin: 40px 0;
		width: 250px;
		height: 40px;
		display: block;
		background: url(../img/logo-admin.png) no-repeat;
	}

	body.ua-sidebar #main-nav { left: 0px; }


.main-nav-toggle {
	border-top: 5px solid #b1bcc7;
	height: 30px;
	width: 32px;
	display: block;
	position: relative;
	top: 10px;
}
	.main-nav-toggle::before {
		content: "";
		background: #b1bcc7;
		height: 5px;
		width: 32px;
		position: absolute;
		top: 5px;
	}
	.main-nav-toggle::after {
		content: "";
		background: #b1bcc7;
		height: 5px;
		width: 32px;
		position: absolute;
		top: 15px;
	}
.main-nav-toggle:hover { border-color: #91a1b1; }
.main-nav-toggle:hover::before,
.main-nav-toggle:hover::after { background: #91a1b1; }


#main-nav .nav a {
	text-decoration: none;
	color: #fff;
	display: block;
	padding: 20px 20px 20px 70px;
}
	#main-nav .nav a:hover { background: #2254a2; }
	#main-nav .nav a.active { background: #142a4e; }

	#main-nav .nav a .icon {
		background: url(../img/icon-dashboard.png) no-repeat;
		width: 27px;
		height: 27px;
		position: absolute;
		left: 30px;
	}
		#main-nav .nav a .icon-reports { background: url(../img/icon-reports.png) no-repeat; }
		#main-nav .nav a .icon-wallboards { background: url(../img/icon-wallboards.png) no-repeat; }
		#main-nav .nav a .icon-alerts { background: url(../img/icon-alerts.png) no-repeat; }
		#main-nav .nav a .icon-agents { background: url(../img/icon-agents.png) no-repeat; }
		#main-nav .nav a .icon-transcriptions { background: url(../img/icon-transcriptions.png) no-repeat; }
		#main-nav .nav a .icon-administration { background: url(../img/icon-administration.png) no-repeat; }








