@charset "utf-8";
/* CSS Document */

*, .nogaps {
	padding:0px;
	margin:0px;
	border:0px;
	outline:0px;
}
html, body {
	height: 100%;
}
body, input, textarea {
	font-family:Tahoma, Arial, Helvetica, sans-serif;
}
a, a:link, a:visited {
	color:#66b0d5;
	text-decoration:none;
}
a:hover, a:active, a:focus {
	color:#5E5E5E;
	outline:none;
}
.account_setup{
	width:25px !important;
}

.add_camp_div
{
	height:30px;
	border-color:#FFF444;
	
}
.test td, ul
{
	margin-bottom: 0px !important;
}
a.active {
	color:#5E5E5E;
}
.quick_src {
	cursor:pointer;
}
p {
	line-height:18px;
}
ul, ol {
	list-style-position: outside;
}
h1, h2, h3, h4, p, ul, ol {
	margin-bottom:20px;
	letter-spacing: normal;
}
h1 {
	font-size:32px;
	clear:both;
	margin-bottom:8px;
	font-family: 'OswaldLight';
	font-weight:normal;
	color:#333;
}
h2 {
	clear:both;
	display:block;
	margin-bottom:8px;
	font-family: 'OswaldLight';
	font-weight:normal;
	font-size:24px;
	color:#333;
}
h3 {
	font-size:1.5em;
	clear:both;
	margin-bottom:15px;
}
h4 {
	clear:both;
	font-size:23px;
	text-transform:uppercase;
	margin-bottom:10px;
	font-weight: normal;
}
.float-r {
	float:right;
}
.float-l {
	float:left;
}
.clear {
	clear:both;
}
	
	
