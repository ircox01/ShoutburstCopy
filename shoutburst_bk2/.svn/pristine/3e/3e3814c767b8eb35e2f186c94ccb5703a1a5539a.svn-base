@font-face {
 font-family: 'OswaldLight';
 src: url('fonts/Oswald-Light-webfont.eot');
 src: url('fonts/Oswald-Light-webfont.eot?#iefix') format('embedded-opentype'),  url('fonts/Oswald-Light-webfont.woff') format('woff'),  url('fonts/Oswald-Light-webfont.ttf') format('truetype'),  url('fonts/Oswald-Light-webfont.svg#OswaldLight') format('svg');
 font-weight: normal;
 font-style: normal;
}
 @font-face {
 font-family: 'OswaldBook';
 src: url('Oswald-Regular-webfont.eot');
 src: url('Oswald-Regular-webfont.eot?#iefix') format('embedded-opentype'),  url('Oswald-Regular-webfont.woff') format('woff'),  url('Oswald-Regular-webfont.ttf') format('truetype'),  url('Oswald-Regular-webfont.svg#OswaldBook') format('svg');
 font-weight: normal;
 font-style: normal;
}
 @font-face {
 font-family: 'OswaldBold';
 src: url('Oswald-Bold-webfont.eot');
 src: url('Oswald-Bold-webfont.eot?#iefix') format('embedded-opentype'),  url('Oswald-Bold-webfont.woff') format('woff'),  url('Oswald-Bold-webfont.ttf') format('truetype'),  url('Oswald-Bold-webfont.svg#OswaldBold') format('svg');
 font-weight: normal;
 font-style: normal;
}
 @font-face {
 font-family: 'OswaldStencilRegular';
 src: url('Oswald-Stencil-webfont.eot');
 src: url('Oswald-Stencil-webfont.eot?#iefix') format('embedded-opentype'),  url('Oswald-Stencil-webfont.woff') format('woff'),  url('Oswald-Stencil-webfont.ttf') format('truetype'),  url('Oswald-Stencil-webfont.svg#OswaldStencilRegular') format('svg');
 font-weight: normal;
 font-style: normal;
}
body {
	font-family:Tahoma, Arial, Helvetica, sans-serif;
	color:#666;
	font-size:13px;
	background-color:#b5e0f6;
}
.wapper {
	min-height: 100%;
	height: auto !important;
	height: 100%;
}
.width-controller {
	width:1240px;
	margin:18px auto;
	display:block;
}
.site-holder {
	background:#f6f6f6;
	-moz-box-shadow: 0 0 6px #cfcfcf;
	-webkit-box-shadow: 0 0 6px #cfcfcf;
	box-shadow: 0 0 6px #cfcfcf;
	width:96%;
	padding:10px 2% 20px;
	float:left;
}
.header {
	min-height:113px;
	background:url(../images/border-image.png) bottom left repeat-x;
	margin-bottom:3px;
}
.header-top {
	float:left;
	width:100%;
	background:url(../images/border-image.png) bottom left repeat-x;
	min-height:60px;
}
.quick-links {
	float:left;
	width:100%;
}
.quick-links ul {
	float:right;
	margin:0px;
	margin-top:10px;
}
.quick-links ul li {
	float:left;
	list-style:none;
	display:block;
}
.quick-links ul li a {
	display:block;
	padding:4px 15px;
	font-size:12px;
}
.quick-links ul li.last a {
	padding-right:0px;
}
.content-wp {
	padding-top:12px;
}
/*Navigaton*/

ul.navigation {
	float:left;
	margin-top:10px;
}
ul.navigation li {
	list-style:none;
	float:left;
	margin:0px 30px;
}
ul.navigation li.last {
	margin-right:0px;
}
ul.navigation li a {
	display:block;
	font-weight:bold;
	font-size:15px;
	padding:6px 0px;
	color:#666666;
	border-bottom:3px solid #d1d1d1;
}
ul.navigation li a:hover, ul.navigation li.active a {
	border-color:#999999;
}
/*End of Navigation*/



.left-col {
	width:250px;
	float:left;
	margin-right:80px;
	min-height:400px;
}
.content-area {
	float:left;
	width:860px;
	min-height:400px;
}
.header-right-content {
	float:right;
	padding-top:5px;
	color:#000;
}
.bottom-area {
	float:left;
	width:100%;
	clear:both;
}
/*Footer*/ 

.footer {
	float:left;
	width:100%;
	margin-top:60px;
	background:url(../images/border-image.png) top left repeat-x;
	padding-top:15px;
}
.copyRightText {
	float:left;
	font-size:11px;
	margin-bottom:0px;
}
.footer-links {
	float:left;
	margin-bottom:0px;
	text-align:center;
	width:850px;
}
.footer-links li {
	list-style:none;
	display: inline;
	margin-right:10px;
}
.footer-links li a {
	color:#555;
	text-decoration:underline;
	font-size:11px;
}
.footer-links li a:hover {
	color:#000;
	text-decoration:none;
}
.social-links {
	float:right;
	margin-bottom:0px;
}
.social-links li {
	float:left;
	list-style:none;
}
.social-links li a {
	width: 34px;
	height: 35px;
	text-indent:-99999px;
	display:block;
	background-image:url(../images/social-links.png);
	background-repeat:no-repeat;
}
.social-links li.facebook a {
	background-position:0px 0px;
}
.social-links li.twitter a {
	margin:0px 10px;
	background-position:-40px 0;
}
.social-links li.in a {
	background-position:-80px 0;
}
.social-links li.facebook a:hover {
	background-position:0px -40px;
}
.social-links li.twitter a:hover {
	background-position:-40px -40px;
}
.social-links li.in a:hover {
	background-position:-80px -40px;
}
/*End of footer*/
	
	
/*Form*/	



.form-item {
	margin-bottom:16px;
	float:left;
	min-width:600px;
	clear:both;
}
.form-item label {
	width:138px;
	float: left;
}
.padd-100 {
	padding-left:138px;
}
input[type="text"],  input[type="password"] {
	border:1px solid #cccccc;
	padding:3px;
	width:180px;
}
a.submit,  input[type="submit"],  input[type="button"] {
	background:#2e2e2e;
	padding:4px 16px 5px;
	color:#fff;
	cursor:pointer;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;
}
input[type="submit"]:hover,  input[type="button"]:hover {
	background:#000;
}
.button-wp {
	width:100%;
}
.account-payment-form {
	float:left;
	width:520px;
}
/*End of form*/

.widget-block {
	width:300px;
	float:left;
}
.widget-head {
	background:#333;
	color:#fff;
	font-size:18px;
	padding:3px 15px;
}
.widget-content {
	padding:8px 15px;
	border:1px solid #ccc;
	min-height:150px;
}
.RHS-content .widget-block {
	float:right;
}
/*Project Listing*/
.priject {
	background:#f9f9f9;
	float:left;
	min-height:230px;
	border:1px solid #e1e1e1;
	padding:11px;
	-moz-border-radius: 8px;
	-webkit-border-radius:8px;
	border-radius:8px;
	margin-bottom:39px;
}
.project-info {
	float: left;
	margin:0px;
	width:100%;
	margin-bottom:20px;
}
.project-info li {
	list-style:none;
	float:left;
	width:172px;
	font-size:11px;
	padding:7px 0px;
}
.project-info li.label {
	color:#999999;
	font-weight:bold;
	width:106px;
}
.image-area {
	float:left;
	width:280px;
	height:140px;
	margin-right:26px;
	overflow:hidden;
	background:#fff;
	text-align:center;
}
.image-area img {
	max-width:280px;
	max-height:140px;
}
.details {
	float:left;
	width:530px;
}
.read-more {
}
.read-more a {
	float:left;
	font-weight:bold;
	-moz-border-radius: 7px;
	-webkit-border-radius:7px;
	border-radius:7px;
	display:block;
	padding:7px 16px;
	border:1px solid #e9e9e9;
	background:#f4f4f4;
	background: -webkit-gradient(linear, left top, left bottom, from(#f4f4f4), to(#f7f7f7));
	background: -moz-linear-gradient(top, #f4f4f4, #f7f7f7);
}
.read-more a span {
	background:url(../images/blue-arrow.png) right center no-repeat;
	padding-right:18px;
}
/*Search form*/

select {
	padding:4px;
	border:1px solid #e1e1e1;
