    <script type="text/javascript">
// generic js functions

function changeCheckboxes(form, checked, prefix, postfix, onlyEnabled) {
  for (var i = 0; i < form.elements.length; i++) {
    var element = form.elements[i];
    if (element.type == "checkbox" && (onlyEnabled == null || (onlyEnabled == true && !element.disabled))) {
      if ((prefix == null || element.name.substr(0, prefix.length) == prefix) 
          && (postfix == null || element.name.substr(element.name.length - postfix.length, element.name.length - 1) == postfix)) {
        element.checked = checked;
      }
    }
  }
}

function toggleCheckbox(checkbox) {
  for (var i = 1; i < toggleCheckbox.arguments.length; i++) {
    var fieldName = toggleCheckbox.arguments[i];
    var dependingField = checkbox.form.elements[fieldName];
    dependingField.disabled = !checkbox.checked;
  }
}

function changeActivation(element, paramName, activationValues) {
  var disable = true;
  for (var i = 2; i < changeActivation.arguments.length; i++) {
    var arg = "" + changeActivation.arguments[i];
    if (element.value == arg || (arg.lastIndexOf("<") == arg.length - 1 && element.value > arg.substr(0, arg.length - 1)))
      disable = false; 
  }
  element.form.elements[paramName].disabled = disable;
}
    
function createHiddenField(form, name, value) {
    var node = document.createElement("input");
    node.setAttribute("type", "hidden");
    node.setAttribute("name", name);
    if (value == null)
        value = "";
    node.setAttribute("value", value);
    form.appendChild(node);
}

function insertText(field, value) {
    if (document.selection) {
        field.focus();
        var range = document.selection.createRange();
        range.text = value;
    }
    else if (field.selectionStart) {
        field.value = field.value.substring(0, field.selectionStart) + value + field.value.substring(field.selectionEnd, field.value.length);
    }
    else {
        field.value += value;
    }
}
function showElement(id, on, inline) {
    var element = document.getElementById(id);
    var visible;
    if (inline == null)
        inline = false;
    if (on == null)
        on = element.style.visibility == "hidden";
    if (on) {
        element.style.visibility = "visible";
        element.style.display = inline ? "inline" : "block";
        visible = true;
    }
    else {
        element.style.visibility = "hidden";
        element.style.display = "none";
        visible = false;
    }
    return visible;
}
