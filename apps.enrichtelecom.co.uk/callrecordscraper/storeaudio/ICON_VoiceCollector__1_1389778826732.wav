<script type="text/javascript">

var detectMSJVM;
var detectSunJVM;   
  
var ua= navigator.userAgent;
var OPERA= (ua.indexOf("Opera") > 0);
var OMNI= (ua.indexOf("Omni") > 0);
var SAFARI= (ua.indexOf("Safari") > 0);
var MAC= (navigator.platform.indexOf("PPC") > 0);  
var WIN= (navigator.platform=="Win32");

var IE6;
var IE7;
var IE5_6;
var IE5_5;
var IE5;
var N4;
var N5;
var NS6;
var NS6_1;
var MOZ;
var MAC_IE5;

if(!OPERA && !OMNI && !SAFARI){
	IE6= (ua.indexOf("MSIE 6") > 0);
	IE7= (ua.indexOf("MSIE 7") > 0);
	// IE 5.5 and IE 5.6 are similar. IE 5.6 is released on WindowsXP
	IE5_6= (ua.indexOf("MSIE 5.6") > 0);
	IE5_5= (ua.indexOf("MSIE 5.5") > 0 || IE5_6);
     
	// IE5 is true for IE5.5, IE5.6, and IE6.
	IE5= (ua.indexOf("MSIE 5") > 0  || IE6 || IE7);
     
	N4= (document.layers);
	NS6= N5= (ua.indexOf("Gecko") > 0);
	NS6_1= (N5 && ua.indexOf("6.1") != -1);
	MOZ= N5 && !(ua.indexOf("Netscape") > 0);
	MAC_IE5= (MAC && IE5);
}

var browserOK = (NS6 || NS6_1 || MOZ || IE5 || IE6 || IE7 || IE5_5 || IE5_6);

if (ua.indexOf("MSIE3")>0) {
	browserOK = false;
}

function browserAlert() {
	if(!browserOK) {
		document.getElementById("WRONG_MS_VERSION").style.visibility = "visible";
	}
}

function detect()
{
	if (browserOK) {
		
			detectSunJVM = document.DetectPluginApplet;
			
			if (navigator.javaEnabled()==true) {
				try {
				    	var test = new String("1.0");
				    	if (detectSunJVM!= null)
					{
				    		test = new String(detectSunJVM.getJavaVersion());
					}
				    	if ( (test.search(/1.3/) != -1)
							     || (test.search(/1.2/) != -1)
							     || (test.search(/1.1/) != -1)
							     || (test.search(/1.0/) != -1))
					{
						document.getElementById("WRONG_SUN_JVM").style.visibility = "visible";
					}	
				 } catch(e) {
				 	document.getElementById("NO_JAVA_ENABLED").style.visibility = "visible";
				 }
			} else {
				 document.getElementById("NO_JAVA_ENABLED").style.visibility = "visible";
			}
		
  	}
}


   function setInputFocus(){
   		if( document.forms != null && document.forms.length > 0 ){
	   		var mainFormElements = document.forms[0].elements;
	   		for( var counter = 0; counter < mainFormElements.length; counter++ )
	   			if( mainFormElements[ counter ].type == "text" ){
	   				mainFormElements[ counter ].focus();
	   				break;
	   			}
	   	}
    }
    function onBodyLoadCallBack (){
		browserAlert();
		//document.write("Is Java enabled? " + navigator.javaEnabled() );
		detect();
		setInputFocus();
    }    
</script>

